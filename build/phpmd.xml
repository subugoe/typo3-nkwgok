  <rule ref="rulesets/unusedcode.xml/UnusedPrivateMethod">
    <priority>3</priority>
  </rule>
  <rule ref="rulesets/unusedcode.xml/UnusedPrivateMethod">
    <priority>3</priority>
  </rule>
</ruleset>


<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:nkwlib/locallang_db.xml:flexform.options</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>

			<source>
				<TCEforms>
					<label>LLL:EXT:nkwgok/pi1/locallang.xml:source</label>
					<config>
						<type>select</type>
						<itemsProcFunc>tx_nkwgok_ff->addFields</itemsProcFunc>
					</config>
				</TCEforms>
			</source>

			<altSource>
				<TCEforms>
					<label>LLL:EXT:nkwgok/pi1/locallang.xml:altSource</label>
					<config>
						<type>input</type>
						<size>30</size>
					</config>
				</TCEforms>
			</altSource>

			<style>
				<TCEforms>
					<label>LLL:EXT:nkwgok/pi1/locallang.xml:style</label>
					<config>
						<type>radio</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">LLL:EXT:nkwgok/pi1/locallang.xml:styleTree</numIndex>
								<numIndex index="1">tree</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">LLL:EXT:nkwgok/pi1/locallang.xml:styleColumn</numIndex>
								<numIndex index="1">column</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">LLL:EXT:nkwgok/pi1/locallang.xml:styleMenu</numIndex>
								<numIndex index="1">menu</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</style>

			<showGOKID>
				<TCEforms>
					<label>LLL:EXT:nkwgok/pi1/locallang.xml:showGOKID</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</showGOKID>

			<omitXXX>
				<TCEforms>
					<label>LLL:EXT:nkwgok/pi1/locallang.xml:omitXXX</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</omitXXX>

		</el>
	</ROOT>
</T3DataStructure>